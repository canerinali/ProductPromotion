@model List<User>

<section class="content">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <!-- /.card-header -->
                <div class="card-body" style="">
                    <table id="example" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>@Html.DisplayNameFor(model => model[0].Id)</th>
                                <th>@Html.DisplayNameFor(model => model[0].Username)</th>
                                <th>@Html.DisplayNameFor(model => model[0].Name)</th>
                                <th>@Html.DisplayNameFor(model => model[0].Surname)</th>
                                <th>@Html.DisplayNameFor(model => model[0].Email)</th>
                                <th>@Html.DisplayNameFor(model => model[0].Password)</th>
                                <th>@Html.DisplayNameFor(model => model[0].CreatedOn)</th>
                                <th>@Html.DisplayNameFor(model => model[0].ModifiedName)</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var user in (List<User>)Model)
                            {
                                <tr>
                                    <td>@user.Id</td>
                                    <td>@user.Username</td>
                                    <td>@user.Name</td>
                                    <td>@user.Surname</td>
                                    <td>@user.Email</td>
                                    <td>@user.Password</td>
                                    <td>@user.CreatedOn</td>
                                    <td>@user.ModifiedName</td>
                                    <td></td>
                                </tr>
                              }
                        </tbody>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<script>
    $(function () {
        $('#example').DataTable({
            "paging": true,
            "lengthChange": true,
            "searching": true,
            "ordering": true,
            "info": true,
            "autoWidth": true,
        });
    });
</script>